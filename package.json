{
  "name": "leser-digital-web-app",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "concurrently -n \"APP, TYPECHECK, GRAPHQL\" \"vite\" \"tsc -b --watch\" \"graphql-codegen --config codegen.yml --watch\"",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "generate": "graphql-codegen --config codegen.yml --watch",
    "svgr": "svgr --typescript --out-dir src/icons src/icons/**/*.svg"
  },
  "dependencies": {
    "@apollo/client": "^3.13.8",
    "@sampled-ui/base": "^0.0.48",
    "formik": "^2.4.6",
    "graphql": "^16.11.0",
    "js-convert-case": "^4.2.0",
    "lucide-react": "^0.509.0",
    "moment": "^2.30.1",
    "node-vibrant": "^4.0.3",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-intersection-observer": "^9.16.0",
    "react-router": "^7.6.0",
    "react-verification-input": "^4.2.2",
    "yup": "^1.6.1"
  },
  "devDependencies": {
    "@0no-co/graphqlsp": "^1.12.16",
    "@eslint/js": "^9.23.0",
    "@graphql-codegen/cli": "^5.0.6",
    "@graphql-codegen/schema-ast": "^4.1.0",
    "@graphql-codegen/typescript-react-apollo": "^4.3.2",
    "@parcel/watcher": "^2.5.1",
    "@svgr/cli": "^8.1.0",
    "@types/react": "^19.1.3",
    "@types/react-dom": "^19.1.3",
    "@types/react-helmet": "^6.1.11",
    "@vitejs/plugin-react-swc": "^3.9.0",
    "classnames": "^2.5.1",
    "concurrently": "^9.1.2",
    "eslint": "^9.26.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-react-hooks": "^5.1.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "globals": "^16.1.0",
    "sass": "^1.88.0",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.32.0",
    "vite": "^6.3.5",
    "vite-plugin-splash-screen": "^0.2.0"
  }
